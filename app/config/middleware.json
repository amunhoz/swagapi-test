{
  
  //===================================================================================
  //express logging
  "morgam": {
    "enabled": false,
    "priority": 1
  },

  //===================================================================================
  //basic configuration for express, gzip, etc.
  "express-basic": {
    "enabled": true,
    "priority": 2
  },

  //===================================================================================
  //start api boot scripts
  "api-init": {
    "enabled": true,
    "priority": 3
  },

 //===================================================================================
  //template system for /views
  // use swagapi.render(tplname);
  "template": {
    "enabled": true,
    "priority": 4
  },
  
  //===================================================================================
  //create a api test interface on http://host:port/explorer
  "swagger-ui-express": {
    "enabled": true,
    "priority": 90
  },

  //===================================================================================
  //serve static files on /public location
  "serve-static": {
    "enabled": true,
    "priority": 91
  },

  //===================================================================================
  //too much security for test enviroment
  "helmet": {
    "enabled": false,
    "priority": 92
  },

  //===================================================================================
  //build routes based on swagger documentation on routesApi
  "swaggerize-express": {
    "enabled": true,
    "priority": 93
  },


  //===================================================================================
  //build routes from routesDir directory files
  "mount-routes": {
    "enabled": true,
    "priority": 94
  },


  //===================================================================================
  //custom express errors with json response
  "express-error": {
    "enabled": true,
    "priority": 95
  },


  //===================================================================================
  //load permissions to routes
  "permissions": {
    "enabled": true,
    "priority": 96
  }

}
